{
  "name": "@safee/gateway",
  "version": "1.0.0",
  "type": "module",
  "main": "build/src/index.js",
  "types": "build/src/index.d.ts",
  "exports": {
    ".": {
      "import": "./build/src/index.js",
      "types": "./build/src/index.d.ts"
    },
    "./auth": {
      "import": "./build/src/auth/index.js",
      "types": "./build/src/auth/index.d.ts"
    }
  },
  "scripts": {
    "build": "npx tsoa spec-and-routes && tsc --build .",
    "dev": "nodemon",
    "start": "node dist/src/server.js",
    "routes": "tsoa routes",
    "spec": "tsoa spec",
    "db:generate": "cd ../database && npm run prisma:generate",
    "db:migrate": "cd ../database && npm run prisma:migrate",
    "db:studio": "cd ../database && npm run prisma:studio",
    "lint": "eslint . --max-warnings 0",
    "lint:fix": "eslint . --max-warnings 0 --fix",
    "test": "vitest run --passWithNoTests",
    "test:watch": "vitest",
    "test:coverage": "vitest run --coverage",
    "check": "tsc --noEmit",
    "fmt": "prettier . --write --cache",
    "fmt:check": "prettier . --check",
    "clean": "tsc --build --clean && rm -rf coverage",
    "secrets:generate": "node ../scripts/generate-secrets.js",
    "secrets:generate:prod": "node ../scripts/generate-secrets.js --env production"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "description": "",
  "engines": {
    "node": ">=20.19.0",
    "npm": ">=10.0.0"
  },
  "dependencies": {
    "@azure/identity": "^4.0.1",
    "@azure/keyvault-secrets": "^4.7.0",
    "@marsaud/smb2": "^0.18.0",
    "@types/multer": "^2.0.0",
    "bcryptjs": "^3.0.2",
    "better-auth": "^1.3.34",
    "cors": "^2.8.5",
    "date-fns": "^4.1.0",
    "dotenv": "^16.4.5",
    "express": "^4.18.2",
    "express-session": "^1.18.0",
    "helmet": "^8.1.0",
    "jsonwebtoken": "^9.0.2",
    "mssql": "^12.0.0",
    "multer": "^2.0.2",
    "mysql2": "^3.15.3",
    "pino": "^9.3.2",
    "pino-http": "^10.1.0",
    "swagger-ui-express": "^5.0.0",
    "tsoa": "^6.6.0",
    "webdav": "^5.8.0",
    "ws": "^8.16.0",
    "zod": "^4.1.12"
  },
  "devDependencies": {
    "@types/bcryptjs": "^3.0.0",
    "@types/cors": "^2.8.19",
    "@types/express": "^5.0.3",
    "@types/express-session": "^1.18.2",
    "@types/jsonwebtoken": "^9.0.10",
    "@types/mssql": "^9.1.8",
    "@types/node": "^24.3.0",
    "@types/supertest": "^6.0.3",
    "@types/swagger-ui-express": "^4.1.6",
    "@types/ws": "^8.5.10",
    "@typescript-eslint/eslint-plugin": "^8.42.0",
    "@typescript-eslint/parser": "^8.42.0",
    "@typescript-eslint/utils": "^8.42.0",
    "@vitest/coverage-v8": "^2.1.8",
    "eslint": "^9.34.0",
    "eslint-plugin-prettier": "^5.5.4",
    "nodemon": "^3.1.10",
    "openapi-types": "^12.1.3",
    "pino-pretty": "^13.1.2",
    "prettier": "^3.6.2",
    "sql-formatter": "^15.3.1",
    "supertest": "^7.1.4",
    "tsx": "^4.20.5",
    "typescript": "^5.9.2",
    "vitest": "^2.1.8"
  }
}
