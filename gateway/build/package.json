{
    "name": "@safee/gateway",
    "version": "1.0.0",
    "type": "module",
    "main": "index.js",
    "scripts": {
        "build": "npx tsoa spec-and-routes && tsc --build .",
        "dev": "nodemon",
        "start": "node dist/src/server.js",
        "routes": "tsoa routes",
        "spec": "tsoa spec",
        "db:generate": "cd ../database && npm run prisma:generate",
        "db:migrate": "cd ../database && npm run prisma:migrate",
        "db:studio": "cd ../database && npm run prisma:studio",
        "lint": "eslint . --max-warnings 0",
        "lint:fix": "eslint . --max-warnings 0 --fix",
        "test": "vitest run --passWithNoTests",
        "test:watch": "vitest",
        "test:coverage": "vitest run --coverage",
        "check": "tsc --noEmit",
        "fmt": "prettier . --write --cache",
        "fmt:check": "prettier . --check",
        "clean": "tsc --build --clean && rm -rf coverage"
    },
    "keywords": [],
    "author": "",
    "license": "ISC",
    "description": "",
    "dependencies": {
        "bcryptjs": "^3.0.2",
        "cors": "^2.8.5",
        "date-fns": "^4.1.0",
        "dotenv": "^16.4.5",
        "express": "^4.18.2",
        "express-session": "^1.18.0",
        "helmet": "^8.1.0",
        "jsonwebtoken": "^9.0.2",
        "pino": "^9.3.2",
        "pino-http": "^10.1.0",
        "swagger-ui-express": "^5.0.0",
        "tsoa": "^6.6.0",
        "ws": "^8.16.0",
        "zod": "^4.0.5"
    },
    "devDependencies": {
        "@types/bcryptjs": "^3.0.0",
        "@types/cors": "^2.8.19",
        "@types/express": "^5.0.3",
        "@types/express-session": "^1.18.2",
        "@types/jsonwebtoken": "^9.0.10",
        "@types/node": "^24.3.0",
        "@types/swagger-ui-express": "^4.1.6",
        "@types/ws": "^8.5.10",
        "@typescript-eslint/eslint-plugin": "^8.42.0",
        "@typescript-eslint/parser": "^8.42.0",
        "@typescript-eslint/utils": "^8.42.0",
        "@vitest/coverage-v8": "^2.1.8",
        "eslint": "^9.34.0",
        "eslint-plugin-prettier": "^5.5.4",
        "nodemon": "^3.1.10",
        "prettier": "^3.6.2",
        "sql-formatter": "^15.3.1",
        "tsx": "^4.20.5",
        "typescript": "^5.9.2",
        "vitest": "^2.1.8"
    }
}
